{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Cw20Deposits",
  "type": "object",
  "required": [
    "amount",
    "contract",
    "count",
    "owner"
  ],
  "properties": {
    "amount": {
      "type": "integer",
      "format": "uint128",
      "minimum": 0.0
    },
    "contract": {
      "type": "string"
    },
    "count": {
      "type": "integer",
      "format": "int32"
    },
    "owner": {
      "type": "string"
    }
  }
}
